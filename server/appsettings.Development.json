{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=postgres;Trusted_Connection=True;MultipleActiveResultSets=true",
    "PostgresConnection": "Host=[hostname];Database=[dbname];Port=5432;Username=[username];Password=[password]",
    "AppID": "[AppID]",
    "AppSecret": "[AppSecret]"
  },

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "Authentication": {
    "Facebook": {
      "AppId": "[AppID]",
      "AppSecret": "[AppSecret]"
    }
  }
}
